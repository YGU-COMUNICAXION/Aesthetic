---
/* Ajusta la ruta de tu layout */
import Layout from "@layouts/Layout.astro";
import { Image } from "astro:assets";

/* Importa tus imágenes reales */
import ExpoHabitat from "@assets/img/galleria/habitat.jpg";
import ExpoMueble from "@assets/img/galleria/mueble.jpg";
import ContactSection from "@components/ContactSection.astro";

type GalleryItem = {
  slug: string;
  title: string; // título visible bajo la imagen
  subtitle: string; // sede + fecha
  image: any; // ImageMetadata
};

const items: GalleryItem[] = [
  {
    slug: "expo-habitat-cdmx-2025",
    title: "EXPO HÁBITAT - CDMX",
    subtitle: "Mayo 2025",
    image: ExpoHabitat,
  },
  {
    slug: "expo-mueble-gdl-2025",
    title: "EXPO MUEBLE - GDL",
    subtitle: "Febrero 2025",
    image: ExpoMueble,
  },
];
---

<Layout title="Galería — Aesthetic Concept Design">
  <!-- Hero / título -->
  <section class="container mx-auto px-4 sm:px-6 lg:px-8 pt-10 md:pt-14">
    <h1
      class="text-center text-2xl md:text-3xl lg:text-4xl text-black font-light leading-relaxed"
    >
      Conoce algunos de los eventos y exhibiciones<br class="hidden sm:block" />
      en los que hemos participado.
    </h1>
  </section>

  <!-- Grid Galería -->
  <section class="container mx-auto px-4 sm:px-6 lg:px-8 py-8 md:py-12 mb-16">
    <div class="grid grid-cols-1 md:grid-cols-2 gap-6 md:gap-8">
      {
        items.map((it) => (
          <figure class="overflow-hidden">
            <Image
              src={it.image}
              alt={`${it.title} — ${it.subtitle}`}
              class="w-full aspect-square object-cover rounded-3xl mb-8"
            />
            <figcaption class="text-center">
              <p class="font-light text-lg lg:text-xl">
                {it.title} <span class="mx-1">-</span> {it.subtitle}
              </p>
            </figcaption>
          </figure>
        ))
      }
    </div>
  </section>

